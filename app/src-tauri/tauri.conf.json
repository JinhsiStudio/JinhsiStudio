{
  "productName": "JinhsiStudio",
  "version": "0.0.5",
  "identifier": "cn.chiichen.JinhsiStudio",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "JinhsiStudio",
        "width": 1200,
        "height": 900,
        "minWidth": 900,
        "minHeight": 600,
        "maxHeight": 1200,
        "maxWidth": 1600
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "default",
        "desktop"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDg1QkFFNkU5MkU3QjA3M0YKUldRL0Izc3U2ZWE2aFdUanpPcndyKzkzWmV5akVkTlY0REIyQlNrS2RmR3M3OHFVWlQydGJuSmQK",
      "endpoints": [
        "https://github.com/JinhsiStudio/JinhsiStudio/releases/latest/download/latest.json"
      ]
    }
  }
}
