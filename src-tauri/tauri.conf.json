{
    "productName": "jinhsi-studio",
    "version": "0.0.1",
    "identifier": "cn.chiichen.JinhsiStudio",
    "build": {
        "beforeDevCommand": "pnpm dev",
        "devUrl": "http://localhost:5173",
        "beforeBuildCommand": "pnpm build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "jinhsi-studio",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}